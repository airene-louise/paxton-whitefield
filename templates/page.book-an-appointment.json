    "content_richtext_Frrgbq": {
      "type": "content_richtext",
      "settings": {
        "content": "<h3>Book your free wedding or celebration cheese cake appointment<\/h3><p>To arrange your free cheese cake consultancy, please call or email your nearest Paxton & Whitfield shop:<\/p><p><strong>London Jermyn Street:<br\/><\/strong>Telephone:<strong> <\/strong><a href=\"https:\/\/www.paxtonandwhitfield.co.uk\/Href=tel:0207930259\" target=\"_blank\">020 7930 0259<\/a><a href=\"https:\/\/www.paxtonandwhitfield.co.uk\/Tel:%20020%207584%200751\"><br\/><\/a><br\/>Email: <a href=\"mailto:jermynstreet@paxtonandwhitfield.co.uk\">jermynstreet@paxtonandwhitfield.co.uk<\/a><\/p><p>Or Book online <a href=\"https:\/\/paxtonandwhitfield.co.uk\/products\/jermyn-street-wedding-appt\" title=\"https:\/\/paxtonandwhitfield.co.uk\/products\/jermyn-street-wedding-appt\">HERE<\/a><\/p><p><strong>London Cale Street: <br\/><\/strong>Telephone:<strong> <\/strong><a href=\"https:\/\/www.paxtonandwhitfield.co.uk\/Tel:%20020%207584%200751\">020 7584 0751<br\/><\/a><br\/>Email: <a href=\"mailto:calestreet@paxtonandwhitfield.co.uk\">calestreet@paxtonandwhitfield.co.uk<\/a><\/p><p><strong>Bath: <br\/><\/strong>Telephone:<strong> <\/strong><a href=\"https:\/\/www.paxtonandwhitfield.co.uk\/Tel:%2001225%20466403\">01225 466403<br\/><\/a><br\/>Email: <a href=\"mailto:bath@paxtonandwhitfield.co.uk\">bath@paxtonandwhitfield.co.uk<\/a><br\/><br\/><strong>Canterbury:<\/strong><br\/>Telephone: <a href=\"tel:01227%20785535\">01227 785535<\/a><br\/>Email: <a href=\"mailto:canterbury@paxtonandwhitfield.co.uk\">canterbury@paxtonandwhitfield.co.uk<\/a><\/p>",
        "use_collection_content": false,
